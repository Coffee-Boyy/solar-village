entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"
certificatesResolvers:
  dns01:
    acme:
      email: {{ dns_email }}
      storage: /etc/traefik/acme.json
      dnsChallenge:
        provider: {{ dns_provider }}
api:
  dashboard: true
providers:
  file:
    directory: /etc/traefik/dynamic
    watch: true